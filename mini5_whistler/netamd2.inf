CopyFiles       = pcntn5m.CopyFiles

[AT2700.ndi.services]
AddService      = PCnet, 2, PCnet5.Service, PCnet.AddEventLog

;-------------------------------------------------------------------------------
; Allied Telesyn AT-2450v4 PCI 10Mb Ethernet Adapter
; 
[AT2701.ndi]
